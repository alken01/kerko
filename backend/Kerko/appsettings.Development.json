{
  "ConnectionStrings": {
    "DefaultConnection": "Server=kerko-db.postgres.database.azure.com;Database=postgres;Port=5432;User Id=kerko_user;Password=password;Ssl Mode=Require;"
  },
  "CORS": {
    "AllowedOrigins": ["http://localhost:3000", "https://localhost:3000"],
    "AllowedMethods": ["GET"]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      },
      "Https": {
        "Url": "https://0.0.0.0:8443",
        "Certificate": {
          "Path": "certificates/certificate.crt",
          "KeyPath": "certificates/private.key"
        }
      }
    }
  },
  "AdminApiKey": "secret-api-key"
}
